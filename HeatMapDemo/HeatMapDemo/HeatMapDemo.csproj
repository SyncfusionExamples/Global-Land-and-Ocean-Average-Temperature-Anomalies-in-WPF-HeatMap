<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net9.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Data\TemperatureData.csv" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Data\TemperatureData.csv" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Syncfusion.SfHeatMap.WPF" Version="*" />
  </ItemGroup>

</Project>
